        <rdfs:comment>Authority component of a URI (e.g. example.com:443).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/onto/ontowebpt#body -->

    <owl:DatatypeProperty rdf:about="http://localhost/onto/ontowebpt#body">
