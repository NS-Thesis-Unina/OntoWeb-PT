<?xml version="1.0"?>
<rdf:RDF xmlns="http://localhost/onto/ontowebpt#"
     xml:base="http://localhost/onto/ontowebpt"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://localhost/onto/ontowebpt">
        <owl:versionIRI rdf:resource="http://localhost/onto/ontowebpt/1.0.1"/>
        <rdfs:comment>OntoWebPT 1.0.1</rdfs:comment>
        <rdfs:label>OntoWebPT</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2008/content#ContentAsBase64 -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2008/content#ContentAsBase64">
        <rdfs:comment>Datatype used to store binary HTTP message bodies encoded in Base64.</rdfs:comment>
        <rdfs:label>Content as Base64</rdfs:label>
    </rdfs:Datatype>



    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://localhost/onto/ontowebpt#aboutVulnerabilityType -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#aboutVulnerabilityType">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Scan"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Vulnerabilities"/>
        <rdfs:comment>Associates a Finding with an abstract vulnerability type.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#detectedByResolver -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#detectedByResolver">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Scan"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Resolver"/>
        <rdfs:comment>Links a Finding to the resolver (Analyzer, Techstack, HTTP) that detected it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#hasHttpFinding -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#hasHttpFinding">
        <owl:inverseOf rdf:resource="http://localhost/onto/ontowebpt#httpFindingOfRequest"/>
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#HttpScan"/>
        <rdfs:comment>Relates an HTTP request to HTTP findings detected on that request.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#httpFindingOfRequest -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#httpFindingOfRequest">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#HttpScan"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Request"/>
        <rdfs:comment>Inverse link: an HttpFinding belongs to a specific Request.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#mthd -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#mthd">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Methods"/>
        <rdfs:comment>Associates an HTTP request with its HTTP method (GET, POST, etc.).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#param -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#param">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Parameter"/>
        <rdfs:comment>Relates a URI to one of its parameters.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#payHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#payHeader">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Message"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#PayloadHeaders"/>
        <rdfs:comment>Links a generic HTTP message to its payload-related headers.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#platformHasVulnerability -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#platformHasVulnerability">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#CPE"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#CVE"/>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#refersToCookie -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#refersToCookie">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#CookieFinding"/>
        <rdfs:comment>Links a cookie-related finding to the cookie it refers to.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#refersToHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#refersToHeader">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#HeaderFinding"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#MessageHeader"/>
        <rdfs:comment>Links a header-related finding to the header it refers to.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#relatedToHTTP -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#relatedToHTTP">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Scan"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#HTTP"/>
        <rdfs:comment>Relates a Finding to an HTTP concept (request, response, header, URI, etc.).</rdfs:comment>
    </owl:ObjectProperty>


    
    <!-- http://localhost/onto/ontowebpt#relatedToHTML -->
    
    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#relatedToHTML">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Scan"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#HTML"/>
        <rdfs:comment>Relates an Analyzer finding to a specific HTML node (Tag/Field).</rdfs:comment>
    </owl:ObjectProperty>



    <!-- http://localhost/onto/ontowebpt#repHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#repHeader">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Message"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#RepresentationHeaders"/>
        <rdfs:comment>Links a message to headers describing the representation (e.g. Content-Type).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#reqHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#reqHeader">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#RequestHeader"/>
        <rdfs:comment>Associates a request with its request-specific headers.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#resHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#resHeader">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Response"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#ResponseHeader"/>
        <rdfs:comment>Associates a response with its response-specific headers.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#resp -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#resp">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Response"/>
        <rdfs:comment>Relates a request to the corresponding response.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#sc -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#sc">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Response"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#StatusCodes"/>
        <rdfs:comment>Links a response to its HTTP status code.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#tagHasProperties -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#tagHasProperties">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Tag"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Field"/>
    </owl:ObjectProperty>
    


    <!-- http://localhost/onto/ontowebpt#uriRequest -->

    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#uriRequest">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#URI"/>
        <rdfs:comment>Associates an HTTP request with the URI it targets.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- http://localhost/onto/ontowebpt#htmlChild -->
    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#htmlChild">
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#HTML"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#HTML"/>
        <rdfs:comment>
            Parent/child relationship between HTML nodes (DOM-like containment).
        </rdfs:comment>
    </owl:ObjectProperty>

    

    <!-- http://localhost/onto/ontowebpt#tagHasChildTag -->
    <owl:ObjectProperty rdf:about="http://localhost/onto/ontowebpt#tagHasChildTag">
        <rdfs:subPropertyOf rdf:resource="http://localhost/onto/ontowebpt#htmlChild"/>
        <rdfs:domain rdf:resource="http://localhost/onto/ontowebpt#Tag"/>
        <rdfs:range rdf:resource="http://localhost/onto/ontowebpt#Tag"/>
        <rdfs:comment>
            Relates an HTML Tag to a nested Tag (e.g. &lt;form&gt; containing &lt;input&gt; elements).
        </rdfs:comment>
    </owl:ObjectProperty>



    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://localhost/onto/ontowebpt#authority -->

    <owl:DatatypeProperty rdf:about="http://localhost/onto/ontowebpt#authority">
