<?xml version="1.0"?>
<rdf:RDF xmlns="http://localhost/ontology/ontowebpt/"
     xml:base="http://localhost/ontology/ontowebpt/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://localhost/ontology/ontowebpt">
        <owl:versionIRI rdf:resource="http://localhost/ontology/ontowebpt/1.1.0"/>
        <rdfs:comment>OntoWebPT 1.1.0 â€” extends 1.0.0 with Finding, Evidence, Resolver, SeverityLevel, FindingSource, and linkage properties to organize vulnerabilities detected from TechStack, Analyzer, and Interceptor.</rdfs:comment>
        <rdfs:label>OntoWebPT</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2008/content#ContentAsBase64 -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2008/content#ContentAsBase64">
        <rdfs:comment>Datatype used to store binary HTTP message bodies encoded in Base64.</rdfs:comment>
        <rdfs:label>Content as Base64</rdfs:label>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://localhost/ontology/ontowebpt#affectsHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#affectsHeader">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#MessageHeader"/>
        <rdfs:comment>Relates a Finding to a specific HTTP header (request/response) impacted by the issue.</rdfs:comment>
        <rdfs:label>affects header</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#affectsParameter -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#affectsParameter">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Parameter"/>
        <rdfs:comment>Relates a Finding to an input parameter (query/path/form) that is affected.</rdfs:comment>
        <rdfs:label>affects parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#affectsRequest -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#affectsRequest">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Request"/>
        <rdfs:comment>Links a Finding to the HTTP request instance it concerns.</rdfs:comment>
        <rdfs:label>affects request</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#affectsResponse -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#affectsResponse">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Response"/>
        <rdfs:comment>Links a Finding to the HTTP response instance it concerns.</rdfs:comment>
        <rdfs:label>affects response</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#affectsURL -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#affectsURL">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:comment>Associates a Finding with the target URL (URI) where the issue manifests.</rdfs:comment>
        <rdfs:label>affects URL</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#categorizedAs -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#categorizedAs">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Vulnerability"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Vulnerabilities"/>
        <rdfs:comment>Classifies a concrete Vulnerability instance against the vulnerability taxonomy (e.g., XSS, SQLi).</rdfs:comment>
        <rdfs:label>categorized as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#detectedBy -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#detectedBy">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Resolver"/>
        <rdfs:comment>Identifies the Resolver (tool/algorithm) that produced the Finding.</rdfs:comment>
        <rdfs:label>detected by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#detectedIn -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#detectedIn">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#FindingSource"/>
        <rdfs:comment>Indicates the data source where the Finding was discovered (TechStack, Analyzer, Interceptor).</rdfs:comment>
        <rdfs:label>detected in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#evidence -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#evidence">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Evidence"/>
        <rdfs:comment>Connects a Finding to one or more Evidence items supporting it (snippets, samples, headers, etc.).</rdfs:comment>
        <rdfs:label>evidence</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#hasSeverity -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#hasSeverity">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#SeverityLevel"/>
        <rdfs:comment>Assigns a qualitative severity level (LOW/MEDIUM/HIGH/CRITICAL) to a Finding.</rdfs:comment>
        <rdfs:label>has severity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#hasVulnerability -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#hasVulnerability">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Vulnerability"/>
        <rdfs:comment>Links a Finding to the concrete Vulnerability instance it evidences.</rdfs:comment>
        <rdfs:label>has vulnerability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#mthd -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#mthd">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Methods"/>
        <rdfs:comment>Relates an HTTP Request to its verb (GET, POST, etc.).</rdfs:comment>
        <rdfs:label>method</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#param -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#param">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Parameter"/>
        <rdfs:comment>Associates a URI with a parameter (name/value pair) extracted from its query or path.</rdfs:comment>
        <rdfs:label>has parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#payHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#payHeader">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Message"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#PayloadHeaders"/>
        <rdfs:comment>Relates a Message to headers that describe the message payload (e.g., Content-Type, Content-Length).</rdfs:comment>
        <rdfs:label>payload header</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#repHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#repHeader">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Message"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#RepresentationHeaders"/>
        <rdfs:comment>Relates a Message to headers describing the selected representation (e.g., Content-Language, Content-Encoding).</rdfs:comment>
        <rdfs:label>representation header</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#reqHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#reqHeader">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#RequestHeader"/>
        <rdfs:comment>Relates an HTTP Request to one of its headers.</rdfs:comment>
        <rdfs:label>request header</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#resHeader -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#resHeader">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Response"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#ResponseHeader"/>
        <rdfs:comment>Relates an HTTP Response to one of its headers.</rdfs:comment>
        <rdfs:label>response header</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#resp -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#resp">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#Response"/>
        <rdfs:comment>Connects a Request to its corresponding Response.</rdfs:comment>
        <rdfs:label>has response</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#sc -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#sc">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Response"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#StatusCodes"/>
        <rdfs:comment>Associates a Response with its status code entity (number and reason phrase).</rdfs:comment>
        <rdfs:label>status code</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://localhost/ontology/ontowebpt#uriRequest -->

    <owl:ObjectProperty rdf:about="http://localhost/ontology/ontowebpt#uriRequest">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:comment>Associates an HTTP Request with the requested target URI.</rdfs:comment>
        <rdfs:label>request URI</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://localhost/ontology/ontowebpt#authority -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#authority">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Authority component of a URI (e.g., host or host:port).</rdfs:comment>
        <rdfs:label>authority</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#body -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#body">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Message"/>
        <rdfs:range rdf:resource="http://www.w3.org/2008/content#ContentAsBase64"/>
        <rdfs:comment>HTTP message body encoded as Base64 for storage and transport safety.</rdfs:comment>
        <rdfs:label>body (Base64)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#connectionAuthority -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#connectionAuthority">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Connection"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Endpoint authority for the TCP/TLS connection used by the HTTP exchange.</rdfs:comment>
        <rdfs:label>connection authority</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#detectedAt -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#detectedAt">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp indicating when the Finding was produced.</rdfs:comment>
        <rdfs:label>detected at</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#evidenceSnippet -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#evidenceSnippet">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Evidence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Short textual snippet illustrating the evidence (e.g., header line, console output).</rdfs:comment>
        <rdfs:label>evidence snippet</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#fieldName -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#fieldName">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#MessageHeader"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the HTTP header field (case-insensitive token).</rdfs:comment>
        <rdfs:label>header name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#fieldValue -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#fieldValue">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#MessageHeader"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Raw value of the HTTP header field.</rdfs:comment>
        <rdfs:label>header value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#fragment -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#fragment">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Fragment component of a URI, without the leading &apos;#&apos;.</rdfs:comment>
        <rdfs:label>fragment</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#hasConfidence -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#hasConfidence">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Finding"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Numeric confidence score in the [0.0, 1.0] range assigned by the resolver.</rdfs:comment>
        <rdfs:label>has confidence</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#hasRemediation -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#hasRemediation">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Recommended remediation or mitigation guidance for the vulnerability.</rdfs:comment>
        <rdfs:label>has remediation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#htmlSnippet -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#htmlSnippet">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Evidence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Excerpt of HTML/DOM source illustrating the issue found by the Analyzer.</rdfs:comment>
        <rdfs:label>HTML snippet</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#httpVersion -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#httpVersion">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Message"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>HTTP protocol version string observed (e.g., HTTP/1.1, HTTP/2).</rdfs:comment>
        <rdfs:label>HTTP version</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#id -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#id">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Request"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Identifier of the captured request provided by the Interceptor.</rdfs:comment>
        <rdfs:label>request id</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#methodName -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#methodName">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Methods"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Human-readable name of an HTTP method (e.g., GET).</rdfs:comment>
        <rdfs:label>method name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#nameParameter -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#nameParameter">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of a parameter associated with a URI.</rdfs:comment>
        <rdfs:label>parameter name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#path -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#path">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Path component of a URI.</rdfs:comment>
        <rdfs:label>path</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#query -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#query">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"/>
        <rdfs:comment>Raw query component of a URI, optionally preserved as XML literal.</rdfs:comment>
        <rdfs:label>query (raw)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#reasonPhrase -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#reasonPhrase">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#StatusCodes"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Textual reason phrase accompanying an HTTP status code (e.g., &quot;OK&quot;).</rdfs:comment>
        <rdfs:label>reason phrase</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#relatedToCPE -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#relatedToCPE">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment>Reference to a Common Platform Enumeration (CPE) URI associated with the vulnerability.</rdfs:comment>
        <rdfs:label>related to CPE</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#relatedToCVE -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#relatedToCVE">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment>Reference to a Common Vulnerabilities and Exposures (CVE) identifier.</rdfs:comment>
        <rdfs:label>related to CVE</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#relatedToCWE -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#relatedToCWE">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment>Reference to a Common Weakness Enumeration (CWE) entry describing the underlying weakness.</rdfs:comment>
        <rdfs:label>related to CWE</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#resolverName -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#resolverName">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Resolver"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Human-readable resolver identifier (e.g., &quot;xss-detector&quot;).</rdfs:comment>
        <rdfs:label>resolver name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#resolverVersion -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#resolverVersion">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#Resolver"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Resolver version string (useful for auditability and reproducibility).</rdfs:comment>
        <rdfs:label>resolver version</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#scheme -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#scheme">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Scheme component of a URI (e.g., http, https).</rdfs:comment>
        <rdfs:label>scheme</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#statusCodeNumber -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#statusCodeNumber">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#StatusCodes"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Numeric HTTP status code (e.g., 200, 404).</rdfs:comment>
        <rdfs:label>status code number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost/ontology/ontowebpt#uri -->

    <owl:DatatypeProperty rdf:about="http://localhost/ontology/ontowebpt#uri">
        <rdfs:domain rdf:resource="http://localhost/ontology/ontowebpt#URI"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Full URI as a string representation.</rdfs:comment>
        <rdfs:label>URI (string)</rdfs:label>
    </owl:DatatypeProperty>
    


